{
    "name": "omnipay/paypal",
    "type": "library",
    "description": "PayPal gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "paypal",
        "purchase"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-paypal",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-paypal/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\PayPal\\" : "src/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true
}
